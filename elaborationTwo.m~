function out=elaborationTwo (image)
    [~, ~, channel] = size(image);
    if channel > 1
     im = rgb2gray (image);
    end
    
    im= im2double(im);
   % im = imadjust(im);
    im=imbinarize(im,'adaptive','ForegroundPolarity','dark','Sensitivity',0.5);
    %im=  imdilate(im, strel ('disk', 4));
    im= imopen(im, strel('disk',6));
    
    out=im;
 
    
    

end
